{
    "Job": {
        "NCPU": 24,
        "parameter_step": 1,
        "queue": "normal",
        "command":"genericpipeline.py -d -c sing_pipeline.cfg $PARSET ",
        "variables": {
            "OBSID": "$OBSID",
            "PIPELINE_STEP": "lb_sub6asec_targ1",
            "RESULTS_DIR": "gsiftp://gridftp.grid.sara.nl:2811/pnfs/grid.sara.nl/data/lofar/user/lofarvwf/disk/ELAIS-N1",
            "ABN": "000",
            "STARTSB": "000",
	    "PICAS_API_VERSION": "spider"
        },
        "wholenode": false
    },
    "Token": {
        "status": "queued",
        "times": {},
        "upload": {
            "add_date": false,
            "date_granularity":"hour",
            "gzip": false,
            "location": "$RESULTS_DIR/$OBSID/step1_subtract_lotss",
            "overwrite": true,
            "template": "$OBSID_lb_sub6asec_ABN$ABN"
        },
        "variables": {
            "_attachments": {
                "PARSET": "step1_subtract_lotss.parset",
                "SRMFILE": "srm.txt"
            },
            "_token_keys": {
                "$OBSID": "OBSID",
                "$PIPELINE_STEP": "PIPELINE_STEP",
                "$RESULTS_DIR": "RESULTS_DIR",
                "$STARTSB": "STARTSB",
                "$ABN": "ABN"
            }
        }
    },
    "container": {
        "singularity": {
            "SIMG": "gsiftp://gridftp.grid.sara.nl:2811/pnfs/grid.sara.nl/data/lofar/user/sksp/distrib/software/lofar_sksp_fedora27_ddf_slurmfix.sif"
        }
    },
    "sandbox": {
        "git": {
            "branch": "prefactor_spider_test",
            "url": "https://git.astron.nl/astron-sdc/GRID_Sandbox.git"
        },
        "scripts": [
            {
                "prefactor": {
                    "branch": "master",
                    "checkout_dir": "pipelines/prefactor",
                    "url": "https://github.com/lofar-astron/prefactor.git"
                }
            }
        ]
    },
    "downloader":{"executable":"globus-url-copy",
        "options":["-rst","-st", "30","-v"],
        "extract":"true"
    }
}
